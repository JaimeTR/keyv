    "json-buffer": "3.0.1"
  },
  "devDependencies": {
    "ava": "latest",
    "coveralls": "latest",
    "eslint-config-xo-lukechilds": "latest",
    "@keyv/mongo": "latest",
    "@keyv/mysql": "latest",
    "@keyv/postgres": "latest",
    "@keyv/redis": "latest",
    "@keyv/sqlite": "latest",
    "@keyv/test-suite": "latest",
    "nyc": "latest",
    "this": "latest",
